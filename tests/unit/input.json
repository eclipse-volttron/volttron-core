{
  "protected_topics": [
    "listener/stats",
    "/devices/*/",
    "/historian_stats/*/"
  ],
  "roles": {
    "edit_config_store": {
      "rpc_capabilities": [
        "config.store.add_config",
        "config.store.delete_config",
        "config.store.edit_config"
      ]
    },
    "admin": {
      "rpc_capabilities": [
        "/.*/"
      ],
      "pubsub_capabilities": {
        "/.*/": "pubsub"
      }
    },
    "test": {
      "pubsub_capabilities": {
        "test": "pubsub"
      }
    }
  },
  "agent_groups": {
    "admin_agents": {
      "identities": [
        "volttron.control",
        "config.store"
      ],
      "agent_roles": [
        "admin"
      ]
    },
    "historians": {
      "identities": [
        "historian1"
      ],
      "rpc_capabilities": [
        "agent1.rpc1"
      ],
      "pubsub_capabilities": {
        "historian_stats/*": "publish"
      }
    }
  },
  "agents": {
    "volttron.control": {
      "comments": "automatically added by platform on start"
    },
    "config.store": {
      "protected_rpcs": [
        "add_config",
        "edit_config",
        "delete_config"
      ],
      "comments": "automatically added by platform on start"
    },
    "listener1": {
      "pubsub_capabilities": {
        "listener/stats": "publish"
      },
      "agent_roles": [
        {
          "edit_config_store": {
            "id": "listener1"
          }
        }
      ],
      "comments": "default entry added at install"
    },
    "platform.driver": {
      "pubsub_capabilities": {
        "devices/*": "publish"
      },
      "agent_roles": [
        {
          "edit_config_store": {
            "id": "platform.driver"
          }
        }
      ]
    },
    "historian1": {
      "pubsub_capabilities": {
        "agent_pubsub_topic": "pubsub"
      },
      "agent_roles": [
        {
          "edit_config_store": {
            "id": "historian1"
          }
        }
      ]
    }
  }
}
